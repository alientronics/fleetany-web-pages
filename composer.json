{
  "name": "alientronics/fleetany-web-pages",
  "description": "Static pages package for Fleetany Web",
  "license": "MIT",
  "authors": [
    {
      "name": "Alientronics"
    }
  ],
  "repositories": [
    {
        "type": "package",
        "package": {
            "version": "dev-master",
            "name": "alientronics/fleetany-web",
            "source": {
                "url": "https://github.com/alientronics/fleetany-web.git",
                "type": "git",
                "reference": "master"
            }
        }
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "laravel/framework": "5.2.*",
    "laravel/laravel": "*",
    "squizlabs/php_codesniffer": "^2.3",
    "phploc/phploc": "*",
    "sebastian/phpcpd": "*",
    "phpmd/phpmd": "*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Alientronics\\FleetanyWebPages\\": "src/Alientronics/FleetanyWebPages"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "vendor/laravel/laravel/app/",
      "Tests\\": "tests/"
    }
  },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('src/routes.php', 'vendor/laravel/laravel/app/Http/routes.php');\"",
            "php -r \"copy('tests/config-app.php', 'vendor/laravel/laravel/config/app.php');\"",
            "mkdir vendor/laravel/laravel/resources/views/layouts/",
            "php -r \"copy('tests/config-app.php', 'vendor/laravel/laravel/resources/views/layouts/login.blade.php');\""
        ]
    },
  "minimum-stability": "dev"
}
